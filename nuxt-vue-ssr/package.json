{
  "name": "nuxt-vue-ssr",
  "private": true,
  "version": "1.0.0",
  "description": "Nuxt.js + Vue Server-Side Rendering Reference App",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:gherkin:component": "playwright test tests/component/gherkin.spec.js",
    "test:gherkin:live": "playwright test tests/live-dependency/gherkin.spec.js",
    "test:gherkin": "npm run test:gherkin:component && npm run test:gherkin:live",
    "docker:build": "docker build -t nuxt-vue-ssr .",
    "docker:run": "docker run -p 3000:3000 nuxt-vue-ssr",
    "docker:compose": "docker-compose up -d"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "nuxt": "^3.8.0",
    "@types/node": "^20.8.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "@vue/test-utils": "^2.4.0",
    "@testing-library/vue": "^7.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "jest-environment-jsdom": "^29.7.0",
    "@playwright/test": "^1.39.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 